<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aigestudio="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".rezervation_activity_with_wheel_time_picker">


    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker_exit_date"

        android:layout_width="180dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="11dp"
        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toStartOf="parent"
        aigestudio:layout_constraintTop_toBottomOf="@+id/textView6"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker_exit_hour"

        android:layout_width="32dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toEndOf="@+id/wheel_picker_exit_date"
        aigestudio:layout_constraintTop_toTopOf="@+id/wheel_picker_exit_date"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker_exit_min"

        android:layout_width="30dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toEndOf="@+id/wheel_picker_exit_hour"
        aigestudio:layout_constraintTop_toTopOf="@+id/wheel_picker_exit_date"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker"

        android:layout_width="180dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:layout_marginStart="76dp"
        android:layout_marginLeft="76dp"
        android:layout_marginTop="8dp"
        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toStartOf="parent"
        aigestudio:layout_constraintTop_toBottomOf="@+id/starter_time"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker2"

        android:layout_width="32dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toEndOf="@+id/wheel_picker"
        aigestudio:layout_constraintTop_toTopOf="@+id/wheel_picker"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <com.aigestudio.wheelpicker.WheelPicker

        android:id="@+id/wheel_picker3"

        android:layout_width="30dp"

        android:layout_height="140dp"

        android:layout_gravity="center"

        android:background="#FFFFFF"
        aigestudio:layout_constraintStart_toEndOf="@+id/wheel_picker2"
        aigestudio:layout_constraintTop_toTopOf="@+id/wheel_picker2"
        aigestudio:wheel_atmospheric="true"
        aigestudio:wheel_curved="true"
        aigestudio:wheel_indicator="true"
        aigestudio:wheel_indicator_color="#D2D2D2"
        aigestudio:wheel_indicator_size="1dp"
        aigestudio:wheel_item_text_color="#A7A7A7"
        aigestudio:wheel_item_text_size="20sp"
        aigestudio:wheel_selected_item_text_color="#333333" />

    <Button
        android:id="@+id/ok_button"
        android:layout_width="101dp"
        android:layout_height="40dp"
        android:layout_marginTop="49dp"
        android:background="@drawable/effect_button"
        android:onClick="ok_button"
        android:text="Check"
        android:textColor="@color/white"
        android:textSize="12sp"
        aigestudio:layout_constraintHorizontal_bias="0.467"
        aigestudio:layout_constraintTop_toBottomOf="@+id/wheel_picker_exit_date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/starter_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="145dp"
        android:layout_marginLeft="145dp"
        android:layout_marginTop="36dp"
        android:text="New Entrance"
        android:textColor="@color/btnColor"
        aigestudio:layout_constraintStart_toStartOf="parent"
        aigestudio:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="167dp"
        android:layout_marginLeft="167dp"
        android:layout_marginTop="62dp"
        android:text="New Exit"
        android:textColor="@color/btnColor"
        aigestudio:layout_constraintStart_toStartOf="parent"
        aigestudio:layout_constraintTop_toBottomOf="@+id/wheel_picker" />


</androidx.constraintlayout.widget.ConstraintLayout>